<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="50.0" prefWidth="200.0" style="-fx-background-color: ffe6fd;" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controladores.TarjetaPopupUsuarioController">
   <children>
      <VBox prefHeight="50.0" prefWidth="200.0">
         <children>
            <JFXButton fx:id="btnEditarPerfil" onAction="#abrirEditarPerfil" prefWidth="200.0" text="Editar Perfil">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </JFXButton>
            <JFXButton fx:id="btnCerrarSesion" onAction="#logout" prefWidth="200.0" text="Cerrar Sesion">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </JFXButton>
         </children>
      </VBox>
   </children>
</AnchorPane>
